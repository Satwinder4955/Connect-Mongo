<h1>{{title}}</h1>
<p>This index is from project</p>


<table>
    <thead>
        <th>Student Name</th>
        <th>StudentID </th>
        <th>Due Date</th>
        <th>Course Name</th>
        <th>Action</th>


    </thead>
    <tbody>
    {{#each dataset}}
    <tr>
        <td>{{this.name}}</td>
        <td>{{this.Id}}</td>
        <td>{{this.date}}</td>
        <td>{{this.course}}</td>
        <td>
            <a href="/projects/edit/{{this._id}}" class="btn btn-primary">Edit</a>
            <a href="/projects/delete/{{this._id}}" onclick="return confirmDeletion()" class="btn btn-danger">Delete</a>
        </td>


    </tr>
    {{/each}}
    </tbody>


</table>
